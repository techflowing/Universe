<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/start_apple_process_activity"
        style="@style/Theme.Universe.Button.List"
        android:layout_marginTop="8dp"
        android:text="启动苹果进程" />

    <Button
        android:id="@+id/start_banana_process_activity"
        style="@style/Theme.Universe.Button.List"
        android:layout_marginTop="8dp"
        android:text="启动香蕉进程" />

    <Button
        android:id="@+id/get_apple_process_service"
        style="@style/Theme.Universe.Button.List"
        android:layout_marginTop="8dp"
        android:text="获取苹果进程服务" />

    <Button
        android:id="@+id/get_banana_process_service"
        style="@style/Theme.Universe.Button.List"
        android:layout_marginTop="8dp"
        android:text="获取香蕉进程服务" />

</LinearLayout>